<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="MainLightColorBrush" Color="#60acd3"/>
    <SolidColorBrush x:Key="MainDarkColorHalfBrush" Color="#7760acd3"/>
    <LinearGradientBrush x:Key="LeftPanelGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#60acd3"  Offset="0"></GradientStop>
        <GradientStop Color="#7760acd3" Offset="1"></GradientStop>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ErrorBrush" Color="#dd1616"/>
</ResourceDictionary>