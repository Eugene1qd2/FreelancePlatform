<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="MainLightColorBrush" Color="#4ECCA3"/>
    <SolidColorBrush x:Key="MainDarkColorHalfBrush" Color="#774ECCA3"/>
    <LinearGradientBrush x:Key="LeftPanelGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#4ECCA3"  Offset="0"></GradientStop>
        <GradientStop Color="#774ECCA3" Offset="1"></GradientStop>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ErrorBrush" Color="#dd1616"/>
</ResourceDictionary>